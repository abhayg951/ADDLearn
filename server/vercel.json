{
    "version": 2.0,
    "builds": [
        {
            "src": "server/app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "env": {
        "APP_MODULE": "app/main.app"
    }
}